{
  "name": "memofee-v2",
  "version": "1.0.0",
  "description": "RSS feed memo web application",
  "main": "dist/server/src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:client -- --watch\" \"nodemon dist/server/index.js\"",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc --project src/server --outDir dist/server",
    "build:client": "tsc --project src/client",
    "start": "npm run build && node dist/server/src/server/index.js",
    "server": "node dist/server/src/server/index.js"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.0.0"
  },
  "keywords": ["rss", "feed", "memo", "typescript"],
  "author": "Yuta Matsumura",
  "license": "MIT"
}